@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:MODSItem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:OriginInfo ;
            owl:onProperty kastle-ont:hasOriginInfo ] .

kastle-ont:ElementInfo a owl:Class .

kastle-ont:AgentRole a owl:Class .

kastle-ont:AuthorityInfo a owl:Class .

kastle-ont:DateInfo a owl:Class .

kastle-ont:DisplayDate a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasDateValue ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasDateValue ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:boolean ;
            owl:onProperty kastle-ont:isContentSuppliedExternally ] .

kastle-ont:IssuanceType.txt a owl:Class .

kastle-ont:OriginInfo a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasPublicationFrequency ] ;
            owl:someValuesFrom kastle-ont:Frequency ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAssoicatedPlace ] ;
            owl:someValuesFrom kastle-ont:Place ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Frequency ;
            owl:onProperty kastle-ont:hasPublicationFrequency ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Place ;
            owl:onProperty kastle-ont:hasAssoicatedPlace ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:DisplayDate ;
            owl:onProperty kastle-ont:hasDisplayDate ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty kastle-ont:hasEventTypeURI ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasEventType ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:DateInfo ;
            owl:onProperty kastle-ont:hasDateInfo ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:IssuanceType.txt ;
            owl:onProperty kastle-ont:hasIssuanceMethod ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Edition ;
            owl:onProperty kastle-ont:hasEdition ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AgentRole ;
            owl:onProperty kastle-ont:hasAssociatedAgentRole ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasEdition ] ;
            owl:someValuesFrom kastle-ont:Edition ],
        kastle-ont:ElementInfo .

kastle-ont:hasEventType a owl:DatatypeProperty .

kastle-ont:hasEventTypeURI a owl:DatatypeProperty .

kastle-ont:hasIssuanceMethod a owl:ObjectProperty .

kastle-ont:Edition a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasEditionValue ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasEditionValue ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:boolean ;
            owl:onProperty kastle-ont:isContentSuppliedExternally ] .

kastle-ont:Frequency a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasFrequencyValue ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasFrequencyValue ;
            owl:someValuesFrom xsd:string ] .

kastle-ont:Place a owl:Class .

kastle-ont:hasAssociatedAgentRole a owl:ObjectProperty .

kastle-ont:hasDateInfo a owl:ObjectProperty .

kastle-ont:hasDateValue a owl:DatatypeProperty .

kastle-ont:hasEditionValue a owl:DatatypeProperty .

kastle-ont:hasFrequencyValue a owl:DatatypeProperty .

kastle-ont:hasOriginInfo a owl:ObjectProperty .

kastle-ont:hasAssoicatedPlace a owl:ObjectProperty .

kastle-ont:hasAuthorityInfo a owl:ObjectProperty .

kastle-ont:hasDisplayDate a owl:ObjectProperty .

kastle-ont:hasPublicationFrequency a owl:ObjectProperty .

kastle-ont:isContentSuppliedExternally a owl:DatatypeProperty .

kastle-ont:hasEdition a owl:ObjectProperty .

kastle-ont:LanguageAttributes a owl:Class .

kastle-ont:hasLanguageAttributes a owl:ObjectProperty .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty kastle-ont:isContentSuppliedExternally ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasEventType ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasLanguageAttributes ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty kastle-ont:isContentSuppliedExternally ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasDisplayDate ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasLanguageAttributes ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasEdition ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AgentRole ;
            owl:onProperty kastle-ont:hasAssociatedAgentRole ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:DisplayDate ;
            owl:onProperty kastle-ont:hasDisplayDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasFrequencyValue ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Place ;
            owl:onProperty kastle-ont:hasAssoicatedPlace ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasDateValue ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasLanguageAttributes ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasDateInfo ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:OriginInfo ;
            owl:onProperty kastle-ont:hasOriginInfo ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasDisplayDate ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAuthorityInfo ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:DateInfo ;
            owl:onProperty kastle-ont:hasDateInfo ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:IssuanceType.txt ;
            owl:onProperty kastle-ont:hasIssuanceMethod ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:anyURI ;
            owl:onProperty kastle-ont:hasEventTypeURI ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAssoicatedPlace ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasEdition ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasPublicationFrequency ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAssociatedAgentRole ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasEditionValue ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Edition ;
            owl:onProperty kastle-ont:hasEdition ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Frequency ;
            owl:onProperty kastle-ont:hasPublicationFrequency ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasOriginInfo ] ] .

