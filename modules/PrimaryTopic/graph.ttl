            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Genre ;
            owl:onProperty kastle-ont:hasGenre ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasTopic ] ;
