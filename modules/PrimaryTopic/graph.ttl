@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:MODSItem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:PrimaryTopic ;
            owl:onProperty kastle-ont:hasPrimaryTopic ] .

kastle-ont:ElementInfo a owl:Class .

kastle-ont:Carographics a owl:Class .

kastle-ont:DateAttributes a owl:Class .

kastle-ont:Genre a owl:Class .

kastle-ont:GeographicSubject a owl:Class .

kastle-ont:Name a owl:Class .

kastle-ont:Usage.txt a owl:Class .

kastle-ont:isPrimaryInstance a owl:ObjectProperty .

kastle-ont:Occupation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasOccupationValue ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasOccupationValue ;
            owl:someValuesFrom xsd:string ] .

kastle-ont:TemporalReference a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasTemporalValue ],
