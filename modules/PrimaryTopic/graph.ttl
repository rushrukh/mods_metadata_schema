            owl:onProperty kastle-ont:hasTemporalReference ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasTitleInfo ] ;
            owl:someValuesFrom kastle-ont:TitleInfo ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
