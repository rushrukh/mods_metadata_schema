@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:Item rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Extension ;
            owl:onProperty kastle-ont:hasExtension ] .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <https://kastle-labl.org/lod/ontology/xsd:string> ;
            owl:onProperty kastle-ont:hasExtensionInfo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasExtension ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Extension ;
            owl:onProperty kastle-ont:hasExtension ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://kastle-labl.org/lod/ontology/xsd:string> ;
            owl:onProperty kastle-ont:hasTypeOfExtension ] .

kastle-ont:Extension rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass <https://kastle-labl.org/lod/ontology/xsd:string> ;
            owl:onProperty kastle-ont:hasExtensionInfo ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasExtensionInfo ;
            owl:someValuesFrom <https://kastle-labl.org/lod/ontology/xsd:string> ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass <https://kastle-labl.org/lod/ontology/xsd:string> ;
            owl:onProperty kastle-ont:hasTypeOfExtension ],
        kastle-ont:ElementInfo .

