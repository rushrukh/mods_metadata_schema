@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
                    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
                    @prefix xml: <http://www.w3.org/XML/1998/namespace> .
                    @prefix modl: <https://archive.org/services/purl/domain/modular_ontology_design_library> .
                    @prefix opla-cp: <http://ontologydesignpatterns.org/opla-cp#> .
                    @prefix opla-sd: <http://ontologydesignpatterns.org/opla-sd#> .
                    @prefix opla-core: <http://ontologydesignpatterns.org/opla-core#> . 
<https://kastle-labl.org/lod/ontology/> rdf:type owl:Ontology ;
                                                                opla-core:hasPatternName "MODSExtension Pattern" ;
                                                                opla-cp:addressesScenario """Use Case
                                                                Use Case Description"""^^xsd:string ;
                                                                opla-cp:hasCompetencyQuestion "cq1"^^xsd:string ,
                                                                                            "cq2"^^xsd:string ;
                                                                opla-sd:hasConnections """MODSItem hasMODSExtension MODSExtension r uie if st
MODSExtension hasTypeOfMODSExtension xsd:string r st
MODSExtension hasMODSExtensionInfo xsd:string r e st
MODSExtension sco ElementInfo"""^^xsd:string ;
                                                                opla-sd:hasSchemaDiagramFileName "MODSExtension.png"^^xsd:string ;
                                                                dc:contributor "Cogan Shimizu" ,
                                                                            "Pascal Hitzler" ,
                                                                            "Rushrukh Rayan" ;
                                                                dc:creator "Cogan Shimizu" ,
                                                                        "Pascal Hitzler" ,
                                                                        "Rushrukh Rayan" .

kastle-ont:MODSItem rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:MODSExtension ;
            owl:onProperty kastle-ont:hasMODSExtension ] .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <https://kastle-labl.org/lod/ontology/xsd:string> ;
            owl:onProperty kastle-ont:hasMODSExtensionInfo ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:MODSExtension ;
            owl:onProperty kastle-ont:hasMODSExtension ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasMODSExtension ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://kastle-labl.org/lod/ontology/xsd:string> ;
            owl:onProperty kastle-ont:hasTypeOfMODSExtension ] .

kastle-ont:MODSExtension rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass xsd:string ;
            owl:onProperty kastle-ont:hasMODSExtensionInfo ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass xsd:string ;
            owl:onProperty kastle-ont:hasTypeOfMODSExtension ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasMODSExtensionInfo ;
            owl:someValuesFrom <https://kastle-labl.org/lod/ontology/xsd:string> ],
        kastle-ont:ElementInfo .

