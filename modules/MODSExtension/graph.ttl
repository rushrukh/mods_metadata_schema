@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:MODSItem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:MODSExtension ;
            owl:onProperty kastle-ont:hasMODSExtension ] .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasTypeOfMODSExtension ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:MODSExtension ;
            owl:onProperty kastle-ont:hasMODSExtension ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasMODSExtensionInfo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasMODSExtension ] ] .

kastle-ont:ElementInfo a owl:Class .

kastle-ont:MODSExtension a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasTypeOfMODSExtension ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasMODSExtensionInfo ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasMODSExtensionInfo ],
        kastle-ont:ElementInfo .

kastle-ont:hasTypeOfMODSExtension a owl:DatatypeProperty .

kastle-ont:hasMODSExtension a owl:ObjectProperty .

kastle-ont:hasMODSExtensionInfo a owl:DatatypeProperty .

