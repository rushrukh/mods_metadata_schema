    owl:someValuesFrom owl:Thing .

[] a owl:Restriction ;
    rdfs:subClassOf kastle-ont:RecordInfo ;
    owl:onProperty kastle-ont:hasDescriptionStandard ;
    owl:someValuesFrom owl:Thing .

