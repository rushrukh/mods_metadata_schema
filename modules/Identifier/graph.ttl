@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:MODSItem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Identifier ;
            owl:onProperty kastle-ont:hasIdentifier ] .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasIdentifier ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty kastle-ont:hasValidity ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Identifier ;
            owl:onProperty kastle-ont:hasIdentifier ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasIdentifierValue ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:anyURI ;
            owl:onProperty kastle-ont:hasIdentifierTypeURI ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:IdentifierType.txt ;
            owl:onProperty kastle-ont:hasIdentifierType ] .

kastle-ont:ElementInfo a owl:Class .

kastle-ont:Identifier a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:IdentifierType.txt ;
            owl:onProperty kastle-ont:hasIdentifierType ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty kastle-ont:hasIdentifierTypeURI ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasIdentifierValue ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasIdentifierValue ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:boolean ;
            owl:onProperty kastle-ont:hasValidity ],
        kastle-ont:ElementInfo .

kastle-ont:IdentifierType.txt a owl:Class .

kastle-ont:hasIdentifierType a owl:ObjectProperty .

kastle-ont:hasIdentifierTypeURI a owl:DatatypeProperty .

kastle-ont:hasValidity a owl:DatatypeProperty .

kastle-ont:hasIdentifier a owl:ObjectProperty .

kastle-ont:hasIdentifierValue a owl:DatatypeProperty .

