@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:MODSItem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:TypeOfResource ;
            owl:onProperty kastle-ont:hasTypeOfResource ] .

kastle-ont:ElementInfo a owl:Class .

kastle-ont:AuthorityInfo a owl:Class .

kastle-ont:TypeOfResource a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasResourceTypeValue ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Usage.txt ;
            owl:onProperty kastle-ont:isPrimaryInstance ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasResourceTypeValue ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:boolean ;
            owl:onProperty kastle-ont:isManuscript ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:boolean ;
            owl:onProperty kastle-ont:isCollection ],
        kastle-ont:ElementInfo .

kastle-ont:Usage.txt a owl:Class .

kastle-ont:isCollection a owl:DatatypeProperty .

kastle-ont:isManuscript a owl:DatatypeProperty .

kastle-ont:isPrimaryInstance a owl:ObjectProperty .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasResourceTypeValue ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasTypeOfResource ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:TypeOfResource ;
            owl:onProperty kastle-ont:hasTypeOfResource ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty kastle-ont:isCollection ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAuthorityInfo ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty kastle-ont:isManuscript ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Usage.txt ;
            owl:onProperty kastle-ont:isPrimaryInstance ] .

kastle-ont:hasResourceTypeValue a owl:DatatypeProperty .

kastle-ont:hasTypeOfResource a owl:ObjectProperty .

kastle-ont:hasAuthorityInfo a owl:ObjectProperty .

