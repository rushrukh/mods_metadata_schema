@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
                    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
                    @prefix xml: <http://www.w3.org/XML/1998/namespace> .
                    @prefix modl: <https://archive.org/services/purl/domain/modular_ontology_design_library> .
                    @prefix opla-cp: <http://ontologydesignpatterns.org/opla-cp#> .
                    @prefix opla-sd: <http://ontologydesignpatterns.org/opla-sd#> .
                    @prefix opla-core: <http://ontologydesignpatterns.org/opla-core#> . 
<https://kastle-labl.org/lod/ontology/> rdf:type owl:Ontology ;
                                                                opla-core:hasPatternName "Term Module Pattern" ;
                                                                opla-cp:addressesScenario """Use Case
                                                                Use Case Description"""^^xsd:string ;
                                                                opla-cp:hasCompetencyQuestion "cq1"^^xsd:string ,
                                                                                            "cq2"^^xsd:string ;
                                                                opla-sd:hasConnections """Thing hasTerm Term r uie if st
Term hasTermType TermType.txt r uie f if st
Term hasAuthorityInfo AuthorityInfo r uie f if st
Term hasLanguageAttributes LanguageAttributes r uie f if st
Term hasAltRepGroup xsd:string r st"""^^xsd:string ;
                                                                opla-sd:hasSchemaDiagramFileName "M_Term.png"^^xsd:string ;
                                                                dc:contributor "Cogan Shimizu" ,
                                                                            "Pascal Hitzler" ,
                                                                            "Rushrukh Rayan" ;
                                                                dc:creator "Cogan Shimizu" ,
                                                                        "Pascal Hitzler" ,
                                                                        "Rushrukh Rayan" .

kastle-ont:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Term ;
            owl:onProperty kastle-ont:hasTerm ] .

kastle-ont:Term rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:TermType.txt ;
            owl:onProperty kastle-ont:hasTermType ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass <https://kastle-labl.org/lod/ontology/xsd:string> ;
            owl:onProperty kastle-ont:hasAltRepGroup ] .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAuthorityInfo ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:TermType.txt ;
            owl:onProperty kastle-ont:hasTermType ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasLanguageAttributes ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasTermType ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Term ;
            owl:onProperty kastle-ont:hasTerm ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://kastle-labl.org/lod/ontology/xsd:string> ;
            owl:onProperty kastle-ont:hasAltRepGroup ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasTermType ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasTerm ] ] .

