@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:Thing a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Term ;
            owl:onProperty kastle-ont:hasTerm ] .

kastle-ont:AuthorityInfo a owl:Class .

kastle-ont:LanguageAttributes a owl:Class .

kastle-ont:Term a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasAltRepGroup ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:TermType.txt ;
            owl:onProperty kastle-ont:hasTermType ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ] .

kastle-ont:TermType.txt a owl:Class .

kastle-ont:hasAltRepGroup a owl:DatatypeProperty .

kastle-ont:hasTerm a owl:ObjectProperty .

kastle-ont:hasAuthorityInfo a owl:ObjectProperty .

kastle-ont:hasLanguageAttributes a owl:ObjectProperty .

kastle-ont:hasTermType a owl:ObjectProperty .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:TermType.txt ;
            owl:onProperty kastle-ont:hasTermType ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasTermType ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:LanguageAttributes ;
            owl:onProperty kastle-ont:hasLanguageAttributes ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Term ;
            owl:onProperty kastle-ont:hasTerm ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAuthorityInfo ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasTerm ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasTermType ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasAltRepGroup ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasLanguageAttributes ] ] .

