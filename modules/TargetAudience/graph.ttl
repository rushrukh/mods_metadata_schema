@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:MODSItem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:TargetAudience ;
            owl:onProperty kastle-ont:hasTargetAudience ] .

kastle-ont:ElementInfo a owl:Class .

kastle-ont:AltFormatAttributes a owl:Class .

kastle-ont:AuthorityInfo a owl:Class .

kastle-ont:TargetAudience a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AltFormatAttributes ;
            owl:onProperty kastle-ont:hasAlternativeFormatAttributes ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasValuAsString ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasValuAsString ;
            owl:someValuesFrom xsd:string ],
        kastle-ont:ElementInfo .

kastle-ont:hasTargetAudience a owl:ObjectProperty .

kastle-ont:hasValuAsString a owl:DatatypeProperty .

kastle-ont:hasAlternativeFormatAttributes a owl:ObjectProperty .

kastle-ont:hasAuthorityInfo a owl:ObjectProperty .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AltFormatAttributes ;
            owl:onProperty kastle-ont:hasAlternativeFormatAttributes ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAlternativeFormatAttributes ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:TargetAudience ;
            owl:onProperty kastle-ont:hasTargetAudience ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasTargetAudience ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasAlternativeFormatAttributes ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAuthorityInfo ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasValuAsString ] .

