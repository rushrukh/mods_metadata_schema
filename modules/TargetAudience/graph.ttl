@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
                    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
                    @prefix xml: <http://www.w3.org/XML/1998/namespace> .
                    @prefix modl: <https://archive.org/services/purl/domain/modular_ontology_design_library> .
                    @prefix opla-cp: <http://ontologydesignpatterns.org/opla-cp#> .
                    @prefix opla-sd: <http://ontologydesignpatterns.org/opla-sd#> .
                    @prefix opla-core: <http://ontologydesignpatterns.org/opla-core#> . 
<https://kastle-labl.org/lod/ontology/> rdf:type owl:Ontology ;
                                                                opla-core:hasPatternName "Target Audience Pattern" ;
                                                                opla-cp:addressesScenario """Use Case
                                                                Use Case Description"""^^xsd:string ;
                                                                opla-cp:hasCompetencyQuestion "cq1"^^xsd:string ,
                                                                                            "cq2"^^xsd:string ;
                                                                opla-sd:hasConnections """MODSItem hasTargetAudience TargetAudience r uie if st
TargetAudience hasAlternativeFormatAttributes AltFormatAttributes r uie f if st
TargetAudience hasAuthorityInfo AuthorityInfo r uie f if st
TargetAudience hasValuAsString xsd:string r e st
TargetAudience sco ElementInfo"""^^xsd:string ;
                                                                opla-sd:hasSchemaDiagramFileName "TargetAudience.png"^^xsd:string ;
                                                                dc:contributor "Cogan Shimizu" ,
                                                                            "Pascal Hitzler" ,
                                                                            "Rushrukh Rayan" ;
                                                                dc:creator "Cogan Shimizu" ,
                                                                        "Pascal Hitzler" ,
                                                                        "Rushrukh Rayan" .

kastle-ont:MODSItem rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:TargetAudience ;
            owl:onProperty kastle-ont:hasTargetAudience ] .

kastle-ont:TargetAudience rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasValuAsString ;
            owl:someValuesFrom <https://kastle-labl.org/lod/ontology/xsd:string> ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass <https://kastle-labl.org/lod/ontology/xsd:string> ;
            owl:onProperty kastle-ont:hasValuAsString ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AltFormatAttributes ;
            owl:onProperty kastle-ont:hasAlternativeFormatAttributes ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        kastle-ont:ElementInfo .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasTargetAudience ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAlternativeFormatAttributes ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AltFormatAttributes ;
            owl:onProperty kastle-ont:hasAlternativeFormatAttributes ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:TargetAudience ;
            owl:onProperty kastle-ont:hasTargetAudience ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://kastle-labl.org/lod/ontology/xsd:string> ;
            owl:onProperty kastle-ont:hasValuAsString ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasAlternativeFormatAttributes ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAuthorityInfo ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasAuthorityInfo ] .

