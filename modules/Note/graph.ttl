@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:MODSItem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Note ;
            owl:onProperty kastle-ont:hasNote ] .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasNote ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasNoteType ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Note ;
            owl:onProperty kastle-ont:hasNote ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasValueAsString ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:anyURI ;
            owl:onProperty kastle-ont:hasNoteTypeURI ] .

kastle-ont:ElementInfo a owl:Class .

kastle-ont:Note a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasNoteType ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasValueAsString ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasValueAsString ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty kastle-ont:hasNoteTypeURI ],
        kastle-ont:ElementInfo .

kastle-ont:hasNoteType a owl:DatatypeProperty .

kastle-ont:hasNoteTypeURI a owl:DatatypeProperty .

kastle-ont:hasNote a owl:ObjectProperty .

kastle-ont:hasValueAsString a owl:DatatypeProperty .

