@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:MODSItem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Abstract ;
            owl:onProperty kastle-ont:hasAbstract ] .

kastle-ont:ElementInfo a owl:Class .

kastle-ont:Abstract a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AltFormatAttributes ;
            owl:onProperty kastle-ont:hasAlternativeFormatAttributes ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:boolean ;
            owl:onProperty kastle-ont:isContentShareable ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasAbstractText ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasAbstractText ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasAbstractType ],
        kastle-ont:ElementInfo .

kastle-ont:AltFormatAttributes a owl:Class .

kastle-ont:hasAbstractType a owl:DatatypeProperty .

kastle-ont:isContentShareable a owl:DatatypeProperty .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty kastle-ont:isContentShareable ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasAbstractText ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasAbstractType ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAlternativeFormatAttributes ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Abstract ;
            owl:onProperty kastle-ont:hasAbstract ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAbstract ] ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasAlternativeFormatAttributes ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AltFormatAttributes ;
            owl:onProperty kastle-ont:hasAlternativeFormatAttributes ] .

kastle-ont:hasAbstract a owl:ObjectProperty .

kastle-ont:hasAbstractText a owl:DatatypeProperty .

kastle-ont:hasAlternativeFormatAttributes a owl:ObjectProperty .

