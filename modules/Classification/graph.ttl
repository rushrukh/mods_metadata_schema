@prefix kastle-ont: <https://kastle-labl.org/lod/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle-ont:MODSItem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Classification ;
            owl:onProperty kastle-ont:hasClassfication ] .

kastle-ont:ElementInfo a owl:Class .

kastle-ont:AuthorityInfo a owl:Class .

kastle-ont:Classification a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasClassificationValueGenerator ],
        [ a owl:Restriction ;
            owl:onProperty kastle-ont:hasValueAsString ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasValueAsString ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:Usage.txt ;
            owl:onProperty kastle-ont:isPrimaryInstance ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onClass kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty kastle-ont:hasEditionAsString ],
        kastle-ont:ElementInfo .

kastle-ont:Usage.txt a owl:Class .

kastle-ont:hasClassificationValueGenerator a owl:DatatypeProperty .

kastle-ont:hasEditionAsString a owl:DatatypeProperty .

kastle-ont:isPrimaryInstance a owl:ObjectProperty .

owl:Thing rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasValueAsString ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:AuthorityInfo ;
            owl:onProperty kastle-ont:hasAuthorityInfo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasClassfication ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Usage.txt ;
            owl:onProperty kastle-ont:isPrimaryInstance ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasClassificationValueGenerator ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass owl:Thing ;
            owl:onProperty [ owl:inverseOf kastle-ont:hasAuthorityInfo ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom kastle-ont:Classification ;
            owl:onProperty kastle-ont:hasClassfication ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty kastle-ont:hasEditionAsString ] .

kastle-ont:hasClassfication a owl:ObjectProperty .

kastle-ont:hasValueAsString a owl:DatatypeProperty .

kastle-ont:hasAuthorityInfo a owl:ObjectProperty .

